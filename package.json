{
  "name": "psa-scr-qst-tst",
  "version": "0.0.440",
  "description": "PisaSales Questionnaire Widgets for Scrivito TEST PACKAGE",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "development": "./src/index.ts"
    },
    "./index.css": {
      "development": "./src/index.scss",
      "default": "./dist/index.css"
    },
    "./editing": {
      "types": "./dist/editing.d.ts",
      "import": "./dist/editing.js",
      "development": "./src/editing.ts"
    },
    "./editing.css": {
      "development": "./src/editing.scss",
      "default": "./dist/editing.css"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js"
    }
  },
  "files": [
    "dist",
    "editing.js",
    "editing.d.ts",
    "editing"
  ],
  "scripts": {
    "build": "vite build && node scripts/postbuild.mjs",
    "dev": "vite dev",
    "watch": "vite build --watch",
    "publish:build": "npm run build && npm publish",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write src"
  },
  "dependencies": {
    "nanoid": "^5.1.5"
  },
  "peerDependencies": {
    "react": ">=18.0.0 <20.0.0",
    "react-dom": ">=18.0.0 <20.0.0",
    "scrivito": "*",
    "scrivito-icon-editor": "^0.0.2"
  },
  "devDependencies": {
    "@types/node": "^22.9.0",
    "@types/react": "^18.2.57",
    "@types/react-dom": "^18.2.21",
    "@types/scrivito": "^1.22.7",
    "@vitejs/plugin-react-swc": "^3.11.0",
    "eslint": "^9.18.0",
    "prettier": "^3.4.2",
    "sass": "^1.83.4",
    "typescript": "^5.7.2",
    "vite": "^6.0.5",
    "vite-plugin-dts": "^4.5.4"
  }
}
